{
  "name": "chingoo.be",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "mkcert": "mkcert local.chingoo.be",
    "ssl": "local-ssl-proxy --config proxy.config.json --key local.chingoo.be-key.pem --cert local.chingoo.be.pem",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:pull": "dotenv -e .env.local -- pnpx prisma db pull",
    "db:schema:gen": "prisma generate",
    "db:schema:format": "prisma-case-format --file ./prisma/schema.prisma --table-case snake --field-case snake --map-table-case pascal --map-field-case camel",
    "db:migrate:gen": "dotenv -e .env.local -- pnpx prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -- pnpx prisma migrate deploy"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@next/third-parties": "^14.1.0",
    "@prisma/client": "^5.9.1",
    "@tabler/icons-react": "^2.47.0",
    "@vercel/postgres": "^0.7.2",
    "clsx": "^2.1.0",
    "html-to-image": "^1.11.11",
    "next": "14.1.0",
    "next-nprogress-bar": "^2.1.2",
    "prisma": "^5.9.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "react-hot-toast": "^2.4.1",
    "server-only": "^0.0.1",
    "superstruct": "^1.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@storybook/addon-essentials": "^7.6.15",
    "@storybook/addon-interactions": "^7.6.15",
    "@storybook/addon-links": "^7.6.15",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.15",
    "@storybook/nextjs": "^7.6.15",
    "@storybook/react": "^7.6.15",
    "@storybook/test": "^7.6.15",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "local-ssl-proxy": "1.3.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma-case-format": "^2.2.1",
    "storybook": "^7.6.15",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
